{
  "name": "eloqua-sdk",
  "version": "1.2.0",
  "description": "SDK to interact with Oracle Eloqua API",
  "main": "dist/Eloqua.js",
  "readmeFilename": "README.md",
  "scripts": {
    "clean": "gulp clean",
    "build": "gulp build",
    "test": "mocha --opts ./mocha.opts test/index.js",
    "test-coverage": "cross-env NODE_ENV=test nyc mocha --opts ./mocha.opts test/index.js",
    "test-verbose": "cross-env DEBUG=eloqua* npm test",
    "lint": "gulp lint",
    "make-docs": "node_modules/.bin/jsdoc -c .jsdoc.json --verbose",
    "release": "./release.sh",
    "coverage": "gulp coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffbaldwinjr/node-eloqua-sdk.git"
  },
  "keywords": [
    "eloqua",
    "oracle",
    "api",
    "bulk",
    "rest"
  ],
  "files": [
    "dist/*"
  ],
  "author": "Jeff Baldwin <jeffbaldwinjr@gmail.com>",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/jeffbaldwinjr/node-eloqua-sdk/issues"
  },
  "homepage": "https://github.com/jeffbaldwinjr/node-eloqua-sdk#readme",
  "engine": {
    "node": ">=7.6"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-istanbul": "^5.1.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "codecov": "^3.1.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "eslint-config-google": "^0.11.0",
    "eslint-plugin-mocha": "^5.2.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-codecov": "^3.0.5",
    "gulp-coveralls": "github:jeffbaldwinjr/gulp-coveralls",
    "gulp-eslint": "^5.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3",
    "mocha": "^4.1.0",
    "must": "^0.13.4",
    "nock": "^10.0.4",
    "nyc": "^13.1.0",
    "release-it": "^10.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "debug": "^4.1.1",
    "deepmerge": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          },
          "exclude": [
            "transform-regenerator"
          ]
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      "@babel/plugin-proposal-throw-expressions",
      "add-module-exports",
      "@babel/plugin-proposal-export-default-from"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      },
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    },
    "comments": false
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": "lcov"
  }
}
